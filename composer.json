{
    "name": "maitiigor/revenue-collection",
    "description": "Payroll application for agencies",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oluwatosinadesoyesalami@gmail.com"
    ],
    "authors": [
        {
            "name": "tosin",
            "email": "oluwatosinadesoyesalami@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.2",
        "maitiigor/foundation-core": "dev-master2"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.5|^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Maitiigor\\RC\\": "src/",
            "Maitiigor\\RC\\Database\\Factories\\": "database/factories/",
            "Maitiigor\\RC\\Database\\Seeders\\": "database/seeders/",
            "Maitiigor\\RC\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maitiigor\\RC\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maitiigor\\RC\\Providers\\RevenueCollectionEventServiceProvider",
                "Maitiigor\\RC\\Providers\\RevenueCollectionServiceProvider",
                "Maitiigor\\RC\\ServiceProvider"
            ],
            "aliases": {
                "RC": "Maitiigor\\RC\\Facades\\RevenueCollection"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../foundation-core"
        }
    ]
}